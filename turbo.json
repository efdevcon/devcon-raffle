{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "env": ["NEXT_VOUCHER_REDEEM_DEADLINE", "NEXT_PUBLIC_INFURA_KEY", "NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID"],
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "build/**", "cache"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "test": {
      "dependsOn": ["build"]
    }
  }
}
