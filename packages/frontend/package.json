{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm next dev",
    "build": "pnpm next build",
    "start": "pnpm next start",
    "lint": "pnpm run lint:ts && pnpm run lint:prettier --check && next lint",
    "lint:fix": "pnpm run lint:prettier --write && pnpm run lint:eslint --fix && pnpm run lint:ts",
    "lint:ts": "tsc --noEmit",
    "lint:prettier": "prettier './{src,__tests__}/**/*.{ts,tsx,graphql}' --cache",
    "lint:eslint": "eslint './{src,__tests__}/**/*.{ts,tsx}' --cache",
    "test:watch": "pnpm vitest",
    "test": "pnpm vitest run"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.28.9",
    "@web3modal/wagmi": "^4.1.9",
    "copy-to-clipboard": "^3.3.3",
    "expiry-set": "^1.0.0",
    "jose": "^5.2.4",
    "lru-cache": "^10.2.1",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "next": "14.1.4",
    "pino": "^8.20.0",
    "react": "^18",
    "react-dom": "^18",
    "styled-components": "^6.1.8",
    "viem": "^2.9.3",
    "wagmi": "^2.5.13",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cookie": "^0.6.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "cookie": "^0.6.0",
    "eslint": "^8.47.0",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^1.13.2",
    "eslint-plugin-react": "^7.34.1",
    "next-test-api-route-handler": "^4.0.7",
    "prettier": "^2.8.8",
    "typescript": "^5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.2"
  }
}
