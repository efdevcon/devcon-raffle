{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm next dev",
    "build": "pnpm next build",
    "start": "pnpm next start",
    "lint": "pnpm run lint:ts && pnpm run lint:prettier --check && next lint",
    "lint:fix": "pnpm run lint:prettier --write && pnpm run lint:eslint --fix && pnpm run lint:ts",
    "lint:ts": "tsc --noEmit",
    "lint:prettier": "prettier './{src,__tests__}/**/*.{ts,tsx,graphql}' --cache",
    "lint:eslint": "eslint './{src,__tests__}/**/*.{ts,tsx}' --cache"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.28.9",
    "@web3modal/wagmi": "^4.1.9",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "next": "14.1.4",
    "react": "^18",
    "react-dom": "^18",
    "styled-components": "^6.1.8",
    "viem": "^2.9.3",
    "wagmi": "^2.5.13"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.47.0",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^1.13.2",
    "eslint-plugin-react": "^7.34.1",
    "prettier": "^2.4.1",
    "typescript": "^5"
  }
}
